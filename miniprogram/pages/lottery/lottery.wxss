.container {
  width: 100%;
  height: 100vh;
  display: flex;
  align-items: center;
  flex-direction: column;
}
.snow-container{
  position: absolute;
  width: 100%;
  overflow: hidden;
  filter: drop-shadow(0 0 10px white);

  height: 100vh;
}

.snow {
  position: absolute;
  width: 10px;
  height: 10px;
  background: white;
  border-radius: 50%;
}
